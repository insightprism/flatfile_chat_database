{
  "flatfile_config": {
    "storage_base_path": "./data_dev",
    "default_chunking_strategy": "optimized_summary",
    "default_embedding_provider": "nomic-ai",
    "lock_retry_delay_ms": 5,
    "lock_retry_max_delay_seconds": 0.5,
    "file_operation_max_retries": 2,
    "file_operation_retry_delay_ms": 5,
    "message_id_length": 8,
    "insight_id_length": 8,
    "temp_file_suffix_length": 6,
    "search_min_word_length": 2,
    "fallback_chunk_size_limit": 300
  },
  "document_processing": {
    "environment_configs": {
      "development": {
        "enable_tracing": true,
        "log_level": "DEBUG",
        "timeout_seconds": 120,
        "max_retries": 1
      }
    },
    "max_concurrent_documents": 2
  },
  "handler_strategies": {
    "strategy_parameters": {
      "chunking": {
        "optimized_summary": {
          "chunk_size": 400,
          "max_tokens_per_chunk": 400,
          "min_tokens_per_chunk": 64
        }
      },
      "embedding": {
        "nomic-ai": {
          "batch_size": 16
        }
      }
    }
  },
  "integration_settings": {
    "performance_settings": {
      "concurrent_documents": 2,
      "vector_batch_size": 25,
      "memory_limit_mb": 512,
      "timeout_per_document_seconds": 600
    },
    "error_handling": {
      "retry_attempts": 1,
      "continue_on_error": false,
      "raise_on_critical_error": true
    },
    "monitoring": {
      "enable_metrics": true,
      "enable_tracing": true,
      "trace_sampling_rate": 1.0,
      "log_performance": true
    }
  },
  "environment": "development"
}